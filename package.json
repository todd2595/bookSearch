{
  "name": "booksearch",
  "version": "1.0.0",
  "description": "## About This Boilerplate",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.6.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/todd2595/bookSearch.git"
  },
  "author": "Todd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/todd2595/bookSearch/issues"
  },
  "homepage": "https://github.com/todd2595/bookSearch#readme"
}
